{
    "name": "walkerchiu/php-account",
    "type": "library",
    "authors": [
        {
            "name":  "Walker Chiu",
            "email": "chenjen.chiou@gmail.com"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3",
        "walkerchiu/php-core": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0"
    },
    "suggest": {
        "walkerchiu/php-currency": "^2.0",
        "walkerchiu/php-morph-address": "^2.0",
        "walkerchiu/php-morph-comment": "^2.0",
        "walkerchiu/php-morph-web": "^2.0",
        "walkerchiu/php-payment": "^2.0",
        "walkerchiu/php-role": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "WalkerChiu\\Account\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WalkerChiu\\Account\\AccountServiceProvider"
            ],
            "aliases": {
                "Profile": "WalkerChiu\\Account\\Models\\Entities\\Profile"
            }
        }
    }
}
